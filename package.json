{
  "name": "css-frameworks-ca",
  "version": "1.0.2",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "test": "jest",
    "format": "prettier --write src/js/*.js src/scss/*.scss",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@eslint/js": "^9.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
