{
  "name": "css-frameworks-ca",
  "version": "1.0.7",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write src/js/*.js src/scss/*.scss",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "test:cypress-open": "cypress open",
    "prepare": "husky install",
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css",
    "start": "live-server",
    "dev": "npm-run-all --parallel watch start build"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@eslint/js": "^9.9.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.13.3",
    "eslint": "^9.9.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-jest": "^28.8.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.html": [
      "prettier --write"
    ],
    "*.scss": "prettier --write"
  }
}
